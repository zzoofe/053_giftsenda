extends common/layout

block content
    main.s-main
        .container
            .b-title
                h1 Sendas
                a.btn.btn_new(href="#")
                    +svg("plus")
                    | new senda
            .b-sep Sendas
            .b-page
                .b-page__frame
                    .b-barTop
                        .b-search
                            input.inp.b-search__inp(type="text" placeholder="Search by recipient, company, campaign")
                            .b-search__icon
                                +svg("search").b-search__icon__search
                                +svg("close").b-search__icon__close
                        .b-filterBtn
                            span Expand the filter
                            +svg("filter")
                    .b-filter
                        .b-filter__status
                            select.js-choice
                                option(placeholder) Status
                                option(value="1") Delivered
                                option(value="2") Need Attention
                                option(value="3") Awaiting Action
                        .b-filter__date
                            div
                                input.inp.inp-date(type="text" placeholder="02/08/20")
                            div
                                input.inp.inp-date(type="text" placeholder="10/08/20")
                    .b-table.b-table_tb1
                        .b-table__bar
                            ul.el-tabs
                                li Active
                                li.is-active Delivered
                                li All
